  <xs:include schemaLocation="https://storage.bindopen.org/xsd/bindopen/IndexedDataItem.xsd" />
  
  <xs:complexType name="BdoExtensionDefinition" abstract="true">
    <xs:complexContent mixed="false">
      <xs:extension base="IndexedDataItem">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="imageUri" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="isEditable" type="xs:boolean" />
          <xs:element minOccurs="0" maxOccurs="1" name="isIndexed" type="xs:boolean" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

</xs:schema>